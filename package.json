{
  "name": "e2e",
  "version": "1.0.0",
  "description": "end-to-end encryption in plain js",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "pm2 start ecosystem.config.json",
    "watch": "pm2 start ecosystem.config.json --watch",
    "stop": "pm2 stop ecosystem.config.json",
    "restart": "pm2 restart ecosystem.config.json",
    "reload": "pm2 reload ecosystem.config.json",
    "delete": "pm2 delete ecosystem.config.json",
    "prettier": "prettier",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "prettier --check \"**/*.ts\"",
    "test": "prettier --check \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soruly/e2e.git"
  },
  "author": "soruly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soruly/e2e/issues"
  },
  "homepage": "https://github.com/soruly/e2e#readme",
  "dependencies": {
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1"
  },
  "devDependencies": {
    "@express-rate-limit/tsconfig": "^1.0.2",
    "@types/node": "^25.2.2",
    "prettier": "^3.8.1"
  }
}
